{
  "name": "bookpoint-v5",
  "private": true,
  "devDependencies": {
    "@wordpress/scripts": "^30.0.0",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "build:book-form": "wp-scripts build blocks/src/book-form/index.js --output-path=blocks/build/book-form",
    "start:book-form": "wp-scripts start blocks/src/book-form/index.js --output-path=blocks/build/book-form",
    "build:admin": "cross-env BABEL_CONFIG_PATH=./babel.config.js wp-scripts build src/admin/index.jsx --output-path=build --output-filename=admin.js",
    "build:front": "cross-env BABEL_CONFIG_PATH=./babel.config.js wp-scripts build src/front/index.jsx --output-path=public --output-filename=front.js",
    "deploy:admin": "powershell -ExecutionPolicy Bypass -File scripts/deploy-admin-build.ps1",
    "build-and-deploy:admin": "npm run build:admin && npm run deploy:admin"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/list": "^6.1.11",
    "@fullcalendar/react": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "@stripe/react-stripe-js": "^5.5.0",
    "@stripe/stripe-js": "^8.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
